  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/speakeasy": "^2.0.7",
    "@types/node": "^20.11.30",
    "rimraf": "^6.0.1",
